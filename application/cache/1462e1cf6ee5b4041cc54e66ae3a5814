$("document").ready(function($){
	$(".accordion").slideUp();
	$('ul').accordion();
	$("#openmobi").click(function(){
		$(".accordion").slideToggle();
  	});

	var nav = $('#primary-navigator');
	$(window).scroll(function () {
		if ($(this).scrollTop() > 125) {
			nav.addClass("f-nav");
		} else {
			nav.removeClass("f-nav");
		}
	});
});
</script>
